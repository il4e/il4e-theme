{
  "name": "il4e-theme",
  "version": "0.0.1",
  "homepage": "http://github.com/il4e/il4e-theme",
  "author": "Michael Greene <profmikegreene@gmail.com>",
  "description": "il4e-theme",
  "main": "css-compiled/theme.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/il4e/il4e-theme.git"
  },
  "files": [
    "dist",
    "docs",
    "src",
    "**/*.scss",
    "gulpfile.js"
  ],
  "license": "MIT",
  "keywords": [
    "css",
    "framework",
    "flexbox",
    "responsive",
    "mobile-friendly",
    "front-end",
    "sass",
    "modern"
  ],
  "bugs": {
    "url": "https://github.com/il4e/il4e-theme/issues"
  },
  "devDependencies": {
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-clean-css": "^3.7.0",
    "gulp-csscomb": "^3.0.8",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "latest"
  },
  "browserslist": [
    "last 4 Chrome versions",
    "Edge >= 12",
    "Firefox ESR",
    "last 4 Safari versions",
    "last 4 Opera versions",
    "Explorer >= 10"
  ],
  "dependencies": {
    "gulp-sourcemaps": "^2.6.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "node-sass --watch -r scss/ -o css-compiled/ --output-style compact",
    "deploy": "node-sass scss/ -o css-compiled/ --output-style compressed",
    "build": "gulp build"
  }
}
